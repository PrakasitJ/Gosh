native_import fmt;

class Person {
    string name;
    int age;

    void Person(string name, int age) {
        this.name = name;
        this.age = age;
    }

	void greet() {
		:: fmt.Println("Hello, my name is", this.name, "and I'm", this.age, "years old");
	}
}

Person p = new Person("John", 30);
p.name = "John";
p.age = 30;
p.greet();